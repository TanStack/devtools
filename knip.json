{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreDependencies": [
    "@faker-js/faker"
  ],
  "ignoreWorkspaces": [
    "examples/**"
  ],
  "workspaces": {
    "packages/devtools-utils": {
      "ignoreDependencies": [
        "react",
        "solid-js",
        "@types/react"
      ],
      "entry": [
        "**/vite.config.solid.ts",
        "**/src/solid/**"
      ],
      "project": [
        "**/vite.config.solid.ts",
        "**/src/solid/**"
      ]
    },
    "packages/solid-devtools": {
      "ignore": [
        "**/core.tsx"
      ]
    }
  }
}